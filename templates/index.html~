    <div id="map"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 41.789680, lng: -87.599754},
          zoom: 18
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAAf9pDhvsm6zO-aAy8cZYZqZNcYwo2oiw&callback=initMap"
    async defer></script>
